{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "no-console": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off", // Allow dangling underscore in variable names. Added during refactoring, consider removing
    "import/no-extraneous-dependencies": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "import/extensions": "off" // Don't show error for specifying file extension while importing .js files. Added during refactoring, consider removing
  }
}
